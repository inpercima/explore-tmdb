{
  "client": {
    "buildDir": "dist",
    "ghUser": "inpercima",
    "installDependencies": true,
    "language": "en",
    "packageJson": {
      "contributors": [{
        "name": "Marcel Jänicke",
        "email": "inpercima@gmail.com"
      }],
      "homepage": "http://explore-tmdb.inpercima.net",
      "repository": "https://github.com/inpercima/explore-tmdb"
    },
    "prefix": "etmdb",
    "routing": {
      "enabled": true,
      "features": {
        "default": "dashboard",
        "name": "features",
        "show": true
      },
      "login": {
        "activate": false,
        "enabled": true,
        "name": "login",
        "show": false
      },
      "notFound": {
        "enabled": true,
        "name": "not-found",
        "redirect": true
      }
    },
    "theme": "indigo-pink",
    "useGoogleFonts": true,
    "useYarn": true
  },
  "general": {
    "author": "Marcel Jänicke",
    "description": "A simple web app allowing users to search on The Movie Database (TMDb) by using the api.",
    "name": "explore-tmdb",
    "title": "explore TMDb",
    "useDocker": true,
    "useMITLicense": true,
    "useMock": false
  },
  "server": {
    "backend": "php",
    "htaccess": true,
    "management": "",
    "packagePath": "",
    "serverAsApi": true
  }
}
