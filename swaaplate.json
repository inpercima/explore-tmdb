{
  "client": {
    "buildDir": "dist",
    "ghUser": "inpercima",
    "installDependencies": true,
    "language": "en",
    "modules": {
      "enabled": true,
      "features": {
        "defaultRoute": "dashboard",
        "name": "features"
      },
      "notFound": {
        "enabled": false,
        "name": ""
      },
      "routing": false
    },
    "packageJson": {
      "contributors": [{
        "name": "Marcel Jänicke",
        "email": "inpercima@gmail.com"
      }],
      "homepage": "http://explore-tmdb.inpercima.net/",
      "repository": "https://github.com/inpercima/explore-tmdb/"
    },
    "prefix": "etmdb",
    "theme": "indigo-pink",
    "useGoogleFonts": true,
    "useYarn": true
  },
  "general": {
    "author": "Marcel Jänicke",
    "description": "A simple web app allowing users to search on The Movie Database (TMDb) by using the api.",
    "modRewriteIndex": true,
    "name": "explore-tmdb",
    "title": "explore TMDb",
    "useDocker": false,
    "useMITLicense": true,
    "useMock": false,
    "useSecurity": false
  },
  "server": {
    "backend": "php",
    "javaKt": {
      "management": "",
      "packagePath": ""
    },
    "php": {
      "modRewritePhpExtension": true,
      "serverAsApi": true
    }
  }
}
